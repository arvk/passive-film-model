  f_env = mu(x,y,z+1)*(exp(mu(x,y,z+1)/(R*T))*(13303/T))
  w_env = 0.0d0

  !! Volume per mole of Fe = 7.122 cc assuming a density of 7.84 g/cc
  !! Number of moles per m^3 = 140401
  f_met = 0.0d0*140401
  w_met = 0.0d0*140401

